# Environment variables for JobTracker docker-compose setup

# PostgreSQL User
# Maps to ${POSTGRES_USER} in docker-compose.yml
POSTGRES_USER=postgres_user

# PostgreSQL Password
# Maps to ${POSTGRES_PASSWORD} in docker-compose.yml
# IMPORTANT: Real secrets must be set in a private .env or CI secrets
POSTGRES_PASSWORD=changeme

# PostgreSQL Database Name
# Maps to ${POSTGRES_DB} in docker-compose.yml
POSTGRES_DB=mydb

# PgAdmin Default Email
# Maps to ${PGADMIN_DEFAULT_EMAIL} in docker-compose.yml
PGADMIN_DEFAULT_EMAIL=admin@example.com

# PgAdmin Default Password
# Maps to ${PGADMIN_DEFAULT_PASSWORD} in docker-compose.yml
# IMPORTANT: Real secrets must be set in a private .env or CI secrets
PGADMIN_DEFAULT_PASSWORD=changeme
